  .container {
    max-width: 1200px;
    margin: 0 auto;
  }
  
  .swiper {
    width: 100%;
  }
  
  .swiper-slide {
    background-color: #f1f1f1;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-items: center;
    border-radius: 8px;
    padding: 15px;
    flex: 0 0 100%;
  }
  
  .slide-image {
    width: 100%;
    border-radius: 5px;
    margin-bottom: 10px;
    aspect-ratio: 3/2;
  -o-object-fit: cover;
  object-fit: cover
  }
  
  .slide-title {
    font-size: 18px;
    font-weight: bold;
    margin-bottom: 5px;
  }
  
  .slide-description {
    font-size: 14px;
    text-align: center;
    color: #555;
  }
  
  .swiper-wrapper{
    display: flex;
    overflow: hidden;
    gap: 20px;
  }
  .swiper-initialized{
    .swiper-wrapper{  
        overflow: unset;
    }
  }
  /* Control slides per view with CSS */
  @media (min-width: 480px) {
    .swiper-slide {
      flex: 0 0 calc((100% - 20px) / 2); /* 2 slides on tablet */
    }
  }
  
  @media (min-width: 768px) {
    .swiper-slide {
      flex: 0 0 calc((100% - 40px) / 3); /* 3 slides on laptop */
    }
  }
  
  @media (min-width: 1200px) {
    .swiper-slide {
      flex: 0 0 calc((100% - 60px) / 4); /* 4 slides on desktop */
    }
  }
  
  .swiper-pagination {
    bottom: 0px;
  }
