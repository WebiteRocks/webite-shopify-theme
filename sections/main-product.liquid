        <script>
            // Increment
            function increment(incrementor, target) {
                var value = parseInt(document.getElementById(target).value, 10);
                value = isNaN(value) ? 0 : value;

                if (incrementor < 0 && value > 1) {
                    value += incrementor;
                } else {
                    value += incrementor;
                }
                document.getElementById(target).value = value;
            } 
        </script>